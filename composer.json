{
    "name": "ferrumfist/yii2-recaptcha-widget",
    "description": "Yii2 Google reCAPTCHA widget",
    "keywords": ["yii2", "captcha", "recaptcha", "google", "widget"],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "source": "https://github.com/ferrumfist/yii2-recaptcha-widget",
        "issues": "https://github.com/ferrumfist/yii2-recaptcha-widget/issues"
    },
    "authors": [
        {
            "name": "ferrumfist",
            "homepage": "https://github.com/ferrumfist/"
        }
    ],
    "require": {
        "yiisoft/yii2": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=3.7",
        "phpunit/dbunit": ">=1.2",
        "phpunit/phpunit-selenium": ">=1.2"
    },
    "autoload": {
        "psr-4": {
            "ferrumfist\\yii2\\recaptcha\\": ""
        }
    }
}
